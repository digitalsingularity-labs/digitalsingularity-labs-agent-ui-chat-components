{
  "name": "@digitalsingularity/agent-ui-chat-components",
  "version": "1.1.1",
  "description": "Reusable UI components for agent chat and management interfaces",
  "type": "module",
  "main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "chat",
    "react",
    "components",
    "ui",
    "typescript",
    "streaming",
    "dependency-injection"
  ],
  "author": "digitalsingularity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalsingularity-labs/digitalsingularity-labs-agent-ui-chat-components"
  },
  "bugs": {
    "url": "https://github.com/digitalsingularity-labs/digitalsingularity-labs-agent-ui-chat-components/issues"
  },
  "homepage": "https://github.com/digitalsingularity-labs/digitalsingularity-labs-agent-ui-chat-components#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.511.0",
    "react-markdown": "^9.0.0"
  }
}
