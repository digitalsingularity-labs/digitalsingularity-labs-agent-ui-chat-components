{
  "name": "@agentui/chat-components",
  "version": "1.0.0",
  "description": "Reusable AI agent chat components with dependency injection for React applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "chat",
    "react",
    "components",
    "ui",
    "typescript",
    "streaming"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/agent-ui-library.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/agent-ui-library/issues"
  },
  "homepage": "https://github.com/yourusername/agent-ui-library#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "dependencies": {
    "react-markdown": "^9.0.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.511.0"
  }
}
